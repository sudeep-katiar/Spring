package com.blblz.spring.SingletonDemo;

public class Human {

	private Heart heart;
	
	public Human()
	{
	}
	
	public Human(Heart heart) {
		this.heart = heart;
	}

	public void setHeart(Heart heart) {
		this.heart = heart;
	}
	
	public void startPumping()
	{
		if(heart != null)
			heart.pump();
		else
			System.out.println("dead");
	}
	
}
